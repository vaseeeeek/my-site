msgid "Basic settings"
msgstr "Базовые настройки"

msgid "Currency"
msgstr "Валюта"

msgid "Weight dimension unit"
msgstr "Единица измерения веса"

msgid "Length dimension unit"
msgstr "Единица измерения длины"

msgid "Service"
msgstr "Название службы"

msgid "Storage time"
msgstr "Срок хранения"

